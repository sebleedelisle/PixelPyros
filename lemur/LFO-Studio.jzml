<SCRIPT name="DoMute(i, x)" script="mutes[i - 1] = x;

if (i == 1) Container.BPContainer1.Breakpoint.mute = x;
if (i == 2) Container.BPContainer2.Breakpoint.mute = x;
if (i == 3) Container.BPContainer3.Breakpoint.mute = x;
if (i == 4) Container.BPContainer4.Breakpoint.mute = x;
if (i == 5) Container.BPContainer5.Breakpoint.mute = x;
if (i == 6) Container.BPContainer6.Breakpoint.mute = x;
if (i == 7) Container.BPContainer7.Breakpoint.mute = x;
if (i == 8) Container.BPContainer8.Breakpoint.mute = x;" trigger_script="" trigger_type="4" trigger="1" osc_message="/DoMute" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="DoSolo(i, x)" script="solos[i - 1] = x;

if (AnySoloOn() == 1) {

	ControlButtons1.Mute.x = !solos[0];	
	ControlButtons2.Mute.x = !solos[1];
	ControlButtons3.Mute.x = !solos[2];
	ControlButtons4.Mute.x = !solos[3];
	ControlButtons5.Mute.x = !solos[4];
	ControlButtons6.Mute.x = !solos[5];
	ControlButtons7.Mute.x = !solos[6];
	ControlButtons8.Mute.x = !solos[7];	

	Container.BPContainer1.Breakpoint.mute = !solos[0];
	Container.BPContainer2.Breakpoint.mute = !solos[1];
	Container.BPContainer3.Breakpoint.mute = !solos[2];
	Container.BPContainer4.Breakpoint.mute = !solos[3];
	Container.BPContainer5.Breakpoint.mute = !solos[4];
	Container.BPContainer6.Breakpoint.mute = !solos[5];
	Container.BPContainer7.Breakpoint.mute = !solos[6];
	Container.BPContainer8.Breakpoint.mute = !solos[7];

} else {

	ControlButtons1.Mute.x = mutes[0];	
	ControlButtons2.Mute.x = mutes[1];
	ControlButtons3.Mute.x = mutes[2];
	ControlButtons4.Mute.x = mutes[3];
	ControlButtons5.Mute.x = mutes[4];
	ControlButtons6.Mute.x = mutes[5];
	ControlButtons7.Mute.x = mutes[6];
	ControlButtons8.Mute.x = mutes[7];	
}
" trigger_script="" trigger_type="4" trigger="1" osc_message="/DoSolo" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="OnLoad()" script="mutes = stretch(0, 8);
solos = stretch(0, 8);

Container.BPContainer1.ccNumber = 21;
Container.BPContainer2.ccNumber = 22;
Container.BPContainer3.ccNumber = 23;
Container.BPContainer4.ccNumber = 24;
Container.BPContainer5.ccNumber = 25;
Container.BPContainer6.ccNumber = 26;
Container.BPContainer7.ccNumber = 27;
Container.BPContainer8.ccNumber = 28;" trigger_script="" trigger_type="5" trigger="1" osc_message="/OnLoad" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="AnySoloOn()" script="decl i;
for (i=0; i<8; i++) {
	if (solos[i] > 0) 
	{
		return 1;
	}
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/AnySoloOn" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
