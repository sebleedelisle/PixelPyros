<SCRIPT name="OnLoad()" script="selection = 0;
currentPresetName = 'Presets';" trigger_script="" trigger_type="5" trigger="1" osc_message="/Container/BPContainer5/PresetMenu/OnLoad" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="OnSelect()" script="decl newName = GetPresetName();

if ((newName != '') && (currentPresetName != newName)) {
	decl len = sizeof(Breakpoint.x);
	decl i;

	decl activeLen = 0;
	decl activeStart = len - 1;
	for (i=0;i<len;i++) {
		if (Breakpoint.pointStatus[i] != -1) {
			activeLen++;
			if (i < activeStart) activeStart = i;
		}
	}
	
	decl activeEnd = (activeLen + activeStart);
	for (i=activeStart;i<activeEnd;i++) {
		Breakpoint.x[i] = (1/(len - 1)) * (i);
		decl y = 0.5;
		decl tempI = i;
		if (newName == 'Sin') {
			y = 0.5 * (1 + sin((i - activeStart) * 2 * pi / (activeLen)));
		} else if (newName == 'Triangle') {	
			tempI = (i - activeStart);
			if (tempI > (activeLen / 2)) tempI = activeEnd - i - (activeLen % 2);
			y = tempI / (floor(activeLen / 2));
		} else if (newName == 'Sawtooth') {
			tempI = ((i - activeStart) % floor(activeLen / 2));
			y = tempI / (floor(activeLen / 2) - 1);
		} else if (newName == 'Square') {
			y = 0;
			if ((tempI - activeStart) % (activeLen / 2) < (activeLen / 4)) y = 1;
		} else if (newName == 'Random') {
			y = rand();
		} else if (newName == 'DrunkWalk') {
			decl lastY = 0.5;
			if (i > 0) lastY = Breakpoint.y[i - 1];
			y = (((rand() - 0.44) * 2) * 0.1) + lastY;
		}
		Breakpoint.y[i] = y;
	}
	currentPresetName = newName;
}" trigger_script="selection" trigger_type="0" trigger="1" osc_message="/Container/BPContainer5/PresetMenu/OnSelect" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="GetPresetName()" script="if (selection == 0) return ''; 

return getattribute(PresetMenu, 'items')[selection];" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer5/PresetMenu/GetPresetName" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="Reset()" script="selection = 0;
currentPresetName = '';" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer5/PresetMenu/Reset" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
</WINDOW>
