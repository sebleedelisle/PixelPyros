<SCRIPT name="Reset()" script="setattribute(Breakpoint, 'nbr', 64);
decl len = sizeof(Breakpoint.x);
decl i;
for (i=0;i<len;i++) {
	Breakpoint.x[i] = (1/(len - 1)) * (i);
	Breakpoint.y[i] = (0.5);
}

ActiveRange.x = {0,1};
UpdateRange(0, 1);
" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/Reset" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="GetLight()" script="decl lightarr, i;
lightarr = stretch(0, Breakpoint.length);
for (i=0; i<Breakpoint.length; i++) {
	lightarr = set(lightarr, GetPointLight(i), i);
}
return lightarr;" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/GetLight" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="Record()" script="if (RecordButton.x != 1) return;

if (recording != 1) recBuffer = stretch(-1, sizeof(x));

if ((recording != 1) && ((recPosition == startPoint))) {
	recording = 1;
	playing = 0;
	setattribute(RecordButton, 'label_on', 'Recording');
} else {
	if (recording != 1) setattribute(RecordButton, 'label_on', 'Waiting');
}

if (recording == 1) {
	decl alreadyLooped = 0;
	if (recBuffer[recPosition] != -1) {
		alreadyLooped = 1;
	} else {
		if (recSource < 3) {
			Breakpoint.y[recPosition] = ((accelerometer[recSource] + 1) / 2);
		}
		recBuffer[recPosition] = y[recPosition];
	}

	if ((alreadyLooped == 1) || (recPosition == Breakpoint.endPoint)) {
		RecordButton.x = 0;
		recording = 0;
		playing = 1;
		Smooth();
	}
}" trigger_script="recPosition" trigger_type="0" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/Record" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="GetPointLight(i)" script="if (recPosition == i) return 2;

if (pointStatus[i] == -1) return -1;

if (length > 4) {
	decl quarter = floor((length) / 4);
	if ((i == quarter) || (i == quarter * 2) || (i == quarter * 3)) return 0.7;
}

return -0.3;" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/GetPointLight" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="Play()" script="if (mute == 1) return;
if (ClockLeds.isStopped == 1) return;
if (Breakpoint.liveMode == 1) {
	y[0] = ((accelerometer[1] + 1) / 2);
	PlayCC(0);
	return;
}

decl pace;

if ((recBars <= 1) && (recording == 1)) {
	pace = ClockLeds.metric_ticks_per_ms * (time - ClockLeds.metric_ms_start) / (1.5 * (64/length) * recBars);
} else {
	pace = (ClockLeds.tick / (1.5 * (64/length) * recBars));
}
pace += OffsetContainer.offsetAmount;
decl pointExact = pace % (endPoint - startPoint + 1);
recPosition = floor(pointExact);
recPosition += startPoint;

if (ClockLeds.tick > 0) {
	PlayCC(pointExact);
}

if (recPosition == startPoint) {	
	ClockLeds.metric_ms_start = time;	
	ClockLeds.metric_ticks_start = ClockLeds.tick;	
	ClockLeds.metric_ms_end = 0;	
	ClockLeds.metric_ticks_end = 0;
} else if (recPosition == endPoint) {	
	ClockLeds.metric_ms_end = time;	
	ClockLeds.metric_ticks_end = ClockLeds.tick;	
	ClockLeds.ticksPerMS();
}" trigger_script="time" trigger_type="0" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/Play" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="PlayCC(atPoint)" script="decl ccValue = (Range.x[0] + ((Range.x[1]-Range.x[0]) * Amount.x) * Breakpoint.y[atPoint]) * 127;
ctlout(0, ccNumber, ccValue, channel);" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/PlayCC" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="PointsDouble()" script="decl oldX = Breakpoint.x;
decl oldY = Breakpoint.y;
decl oldLen = sizeof(oldY);
if (oldLen == 64) return;

decl newLen = (oldLen * 2);
if (newLen > 64) {
		newLen = 64;
}

decl i;
setattribute(Breakpoint, 'nbr', newLen);
//Reset();
UpdateRange(ActiveRange.x[0], ActiveRange.x[1]);
for (i=0; i<oldLen; i++) {
	Breakpoint.y[2*i] = oldY[i];
	Breakpoint.x[2*i] = oldX[i];
	if ((i + 1) < oldLen) {
		Breakpoint.y[2*i + 1] = (oldY[i] + oldY[i + 1]) / 2;
		Breakpoint.x[2*i + 1] = (oldX[i] + oldX[i + 1]) / 2;
	} else {
		Breakpoint.y[2*i + 1] = (oldY[i] + oldY[0]) / 2;
		Breakpoint.x[2*i + 1] = (oldX[i]);
	}
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/PointsDouble" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="PointsHalve()" script="if (length <= minPointLimit) return;

decl oldY = Breakpoint.y;
decl maxY = GetMax(Breakpoint.y);
decl oldX = Breakpoint.x;
decl maxX = GetMax(Breakpoint.x);
decl newLastX = Breakpoint.x[length - 2];
decl diff = (maxX / newLastX);

decl i;
decl len = floor(length * 0.5);

setattribute(Breakpoint, 'nbr', len);
//Reset();
UpdateRange(ActiveRange.x[0], ActiveRange.x[1]);
for (i=0; i<len; i++) {
	Breakpoint.y[i] = oldY[(i * 2 * diff)];
	Breakpoint.x[i] = oldX[(i * 2 * diff)];
}" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/PointsHalve" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="UpdateRange(lo, hi)" script="decl i;
if (sizeof(pointStatus) != length) {
	pointStatus = stretch(0, length);
}

if (hi == 0) {
	liveMode = 1;
	pointStatus = set(stretch(-1, length), 0, 0);
	return;
} else {
	liveMode = 0;
}

startPoint = floor(lo * length);
endPoint = floor(hi * length) - 1;
for (i=0; i<length; i++) {
	if (((i/length) < lo) || ((i/length) > hi)) 
	{
		pointStatus[i] = -1;
	} else {
		pointStatus[i] = 0;
	}
}
" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/UpdateRange" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="Smooth()" script="decl i, len = sizeof(recBuffer);

for(i=0; i<len; i++) {
	if (recBuffer[i] == -1) {
		decl prevVal = GetPrevRecBuffer(i);
		decl nextVal = GetNextRecBuffer(i);
		if ((prevVal == -1) || (nextVal == -1)) return;
		decl newVal = (prevVal + nextVal) / 2;
		y[i] = newVal;
	}
}
" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/Smooth" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="GetPrevRecBuffer(i)" script="decl j, count, len = sizeof(recBuffer);
j = ((i + len) - 1) % len;
while (recBuffer[j] == -1) {
	j = ((j + len) - 1) % len;
	count += 1;
	if (count == len) return -1;
}
return recBuffer[j];" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/GetPrevRecBuffer" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
<SCRIPT name="GetNextRecBuffer(i)" script="decl j, count, len = sizeof(recBuffer);
j = ((i + len) + 1) % len;
while (recBuffer[j] == -1) {
	j = ((j + len) + 1) % len;
	count += 1;
	if (count == len) return -1;
}
return recBuffer[j];" trigger_script="" trigger_type="4" trigger="1" osc_message="/Container/BPContainer6/Breakpoint/GetNextRecBuffer" midi_message="0x90,0x90,0,0" midi_target="-1" flag="1"/>
